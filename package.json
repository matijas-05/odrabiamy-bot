{
	"name": "odrabiamy-bot",
	"version": "1.7.6",
	"description": "Discord bot for retrieving data from https://odrabiamy.pl/",
	"main": "src/main.ts",
	"scripts": {
		"start": "if [ \"$NODE_ENV\" = \"server\" ]; then ts-node --transpile-only src/main.ts; fi",
		"dev": "export NODE_ENV=local && nodemon --files src/main.ts",
		"commands": "ts-node src/commands-deploy.ts",
		"commands-dev": "ts-node src/commands-deploy-dev.ts",
		"logout": "rm src/config/cookies.json",
		"global": "npm i -g nodemon"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/matijas-05/odrabiamy-bot.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/matijas-05/odrabiamy-bot/issues"
	},
	"homepage": "https://github.com/matijas-05/odrabiamy-bot#readme",
	"devDependencies": {
		"@types/cli-color": "^2.0.2",
		"@types/express": "^4.17.14",
		"@types/fs-extra": "^9.0.13",
		"@types/puppeteer": "^5.4.7",
		"@typescript-eslint/eslint-plugin": "^5.42.1",
		"@typescript-eslint/parser": "^5.42.1"
	},
	"dependencies": {
		"@discordjs/builders": "^1.3.0",
		"@discordjs/rest": "^1.3.0",
		"cli-color": "^2.0.3",
		"discord-api-types": "^0.37.17",
		"discord.js": "^14.6.0",
		"dotenv": "^16.0.3",
		"express": "^4.18.2",
		"fs-extra": "^10.1.0",
		"puppeteer": "^19.2.2",
		"puppeteer-extra": "^3.3.4",
		"puppeteer-extra-plugin-stealth": "^2.11.1",
		"ts-node": "^10.9.1"
	}
}
