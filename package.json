{
	"name": "odrabiamy-bot",
	"version": "1.1.0",
	"description": "Discord bot for retrieving data from https://odrabiamy.pl/",
	"main": "src/main.ts",
	"scripts": {
		"start": "ts-node src/main.ts",
		"dev": "ts-node-dev --respawn src/main.ts",
		"build": "ts-node src/commands-deploy.ts ",
		"logout": "rm -r user_data",
		"deploy": "pm2 --name bot start npm -- start",
		"undeploy": "pm2 delete 0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/matijas-05/odrabiamy-bot.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/matijas-05/odrabiamy-bot/issues"
	},
	"homepage": "https://github.com/matijas-05/odrabiamy-bot#readme",
	"devDependencies": {
		"@types/fs-extra": "^9.0.13",
		"@types/puppeteer": "^5.4.6",
		"@typescript-eslint/eslint-plugin": "^5.34.0",
		"@typescript-eslint/parser": "^5.34.0",
		"ts-node": "^10.9.1",
		"ts-node-dev": "^2.0.0"
	},
	"dependencies": {
		"@discordjs/builders": "^1.2.0",
		"@discordjs/rest": "^1.1.0",
		"discord-api-types": "^0.37.4",
		"discord.js": "^14.3.0",
		"fs-extra": "^10.1.0",
		"puppeteer": "^16.2.0",
		"puppeteer-extra": "^3.3.4",
		"puppeteer-extra-plugin-stealth": "^2.11.1"
	}
}
