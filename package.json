{
	"name": "odrabiamy-bot",
	"version": "1.0.0-rc3",
	"description": "Discord bot for retreiving data from https://odrabiamy.pl/",
	"main": "src/main.ts",
	"scripts": {
		"start": "ts-node src/main.ts",
		"dev": "ts-node-dev --respawn src/main.ts",
		"build": "ts-node src/commands-deploy.ts ",
		"clear": "rm -r user_data && rm src/config/cookies.json",

		"deploy": "pm2 --name bot start npm -- start",
		"undeploy": "pm2 delete 0"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/matijas-05/odrabiamy-bot.git"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"bugs": {
		"url": "https://github.com/matijas-05/odrabiamy-bot/issues"
	},
	"homepage": "https://github.com/matijas-05/odrabiamy-bot#readme",
	"devDependencies": {
		"@types/puppeteer": "^5.4.5",
		"ts-node": "^10.6.0",
		"ts-node-dev": "^1.1.8"
	},
	"dependencies": {
		"@discordjs/builders": "^0.12.0",
		"@discordjs/rest": "^0.3.0",
		"discord-api-types": "^0.27.3",
		"discord.js": "^13.6.0",
		"puppeteer": "^13.4.1",
		"puppeteer-extra": "^3.2.3",
		"puppeteer-extra-plugin-stealth": "^2.9.0"
	}
}
